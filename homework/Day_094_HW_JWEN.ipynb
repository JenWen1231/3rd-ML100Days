          "base_uri": "https://localhost:8080/",
          "height": 34
        }
      },
      "source": [
        "np.random.seed(1)\n",
        "a_slice_prev=np.random.randn(8,8,3)\n",
        "W=np.random.randn(8,8,3)\n",
        "b=np.random.randn(1,1,1)\n",
        "Z=conv_single_step(a_slice_prev,W,b)\n",
        "print('Z=',Z)"
