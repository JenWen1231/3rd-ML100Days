      "source": [
        "def conv_single_step(a_slice_prev,W,b):\n",
        "  s=a_slice_prev*W\n",
        "  Z=np.sum(s)\n",
        "  Z=float(Z+b)\n",
        "  return Z"
      ],
      "execution_count": 0,
      "outputs": []
    },
